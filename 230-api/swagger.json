{ "openapi":"3.0.0",
  "info":{
    "title":"CAB230 - Web Computing API",
    "description":"This is Samuel Holden's CAB230 server side API for final assessment",
    "version":"1.0.0", "termsOfService":"http://swagger.io/terms/", "contact":{"email":"n10110381@qut.edu.au"}, "license":{
      "name":"Apache 2.0",
      "url":"http://www.apache.org/licenses/LICENSE-2.0.html" }
  },
  "servers":[
    {"url": "https://localhost"},
    {"url": "https://172.22.24.250"}
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "JsonWebToken in order to access secure search route"
      }
    }
  },
  "paths": {
    "/register": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Register a new user account",
        "description": "Registers a new account, for use of the secure search route",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "required": true,
                    "example": "user@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "required": true,
                    "example": "userPassword"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Account successfully created!"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Error - User already exists"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/login": {
      "post": {
        "summary": "Login with an existing user account",
        "description": "Login to an existing account, in order to use the secure search route",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "required": true,
                    "example": "user@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "required": true,
                    "example": "userPassword"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJlbWFpbCI6InRlc3QiLCJpYXQiOjE1NTg1MTc1MTgsImV4cCI6MTU1ODUxNzUzOH0.F2fjmSUq1p3e1kyOefm8tHSNoZDhND5KYm1HWJIytAE"
                    },
                    "token_type": {
                      "type": "string",
                      "example": "Bearer"
                    },
                    "expires_in": {
                      "type": "integer",
                      "example": 21600
                    },
                    "message": {
                      "type": "string",
                      "example": "Successfully logged in!"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "User credentials do not match any of our records"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Authentication"
        ]
      }
    },
    "/search": {
      "get": {
        "description": "Search for Offence data",
        "parameters": [
          {
            "name": "offence",
            "in": "query",
            "description": "Name of the Offence to search for",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "area",
            "in": "query",
            "description": "List of comma-separated Areas (LGAs) to filter search results by",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "age",
            "in": "query",
            "description": "List of comma-separated Ages to filter search results by",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "gender",
            "in": "query",
            "description": "List of comma-separated Genders to filter search results by",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "year",
            "in": "query",
            "description": "List of comma-separated Years to filter search results by",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "month",
            "in": "query",
            "description": "List of comma-separated Months to filter search results by (1-12)",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "security": [{"bearerAuth": []}],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "query": {
                      "type": "object",
                      "properties": {
                        "offence": {
                          "type": "string",
                          "example": "Homicide (Murder)"
                        }
                      }
                    },
                    "result": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "LGA": {
                            "type": "string",
                            "example": "Aurukun Shire Council"
                          },
                          "total": {
                            "type": "integer",
                            "example": 4
                          },
                          "lat": {
                            "type": "decimal(10,8)",
                            "example": -27.470812
                          },
                          "lng": {
                            "type": "decimal(11,8)",
                            "example": 153.022455
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "You must present a valid offence to search"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Authorization token has expired and/or is invalid"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Search"
        ]
      }
    },
    "/offences": {
      "get": {
        "description": "List of offences to perform searches for",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "offences": {
                      "type": "array",
                      "example": [
                        "Advertising Prostitution",
                        "Armed Robbery"
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Helpers"
        ]
      }
    },
    "/areas": {
      "get": {
        "description": "List of Areas to filter search results by",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "areas": {
                      "type": "array",
                      "example": [
                        "Aurukun Shire Council",
                        "Balonne Shire Council"
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Helpers"
        ]
      }
    },
    "/ages": {
      "get": {
        "description": "List of Ages to filter search results by",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ages": {
                      "type": "array",
                      "example": [
                        "Adult",
                        "Juvenile"
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Helpers"
        ]
      }
    },
    "/genders": {
      "get": {
        "description": "List of Genders to filter search results by",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "genders": {
                      "type": "array",
                      "example": [
                        "Female",
                        "Male"
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Helpers"
        ]
      }
    },
    "/years": {
      "get": {
        "description": "List of Years to filter search results by",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "years": {
                      "type": "array",
                      "example": [
                        2001,
                        2002
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Helpers"
        ]
      }
    }
  }
}